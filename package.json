{
    "name": "vscode-unfancy-file-icons",
    "displayName": "Unfancy file icons",
    "description": "Octicons as file icons",
    "categories": [
        "Themes"
    ],
    "version": "1.2.0",
    "publisher": "alexesprit",
    "license": "MIT",
    "engines": {
        "vscode": "*"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/alexesprit/vscode-unfancy-file-icons.git"
    },
    "contributes": {
        "iconThemes": [
            {
                "id": "vscode-unfancy-file-icons",
                "label": "Unfancy file icons",
                "path": "./resources/icons.json"
            }
        ]
    },
    "dependencies": {
        "color": "^3.1.2",
