      "mp4",
      "mpg",
      "ogg",
      "swf",
      "wma"
    ],
    "notebook": ["ipynb"],
    "pdf": ["pdf"],
    "ruby": ["gemspec", "podspec", "rake", "rb", "rbi", "rbx", "ru"],
    "search": ["code-search"],
    "sheet": ["csv", "tsv"],
    "src": [
      "c",
      "c++",
      "cc",
      "cjs",
      "clj",
      "cljc",
      "cljs",
      "cljx",
      "coffee",
      "cpp",
      "cpy",
      "cr",
      "cs",
      "csx",
      "cxx",
      "es",
      "es6",
      "ex",
      "exs",
      "fs",
      "fsi",
      "fsscript",
      "fsx",
      "go",
      "groovy",
      "gs",
      "gvy",
      "hlsl",
      "hs",
      "htc",
      "hx",
      "hxsl",
      "iced",
      "ipy",
      "java",
      "js",
      "jscad",
      "jsm",
      "json5",
      "jsx",
      "litcoffee",
      "lua",
      "m",
      "mjs",
      "mm",
      "nqp",
      "p6",
      "pac",
      "pjs",
      "pl",
      "pl6",
      "pm",
      "pm6",
      "psgi",
      "py",
      "pyw",
      "r",
      "rpy",
      "rs",
      "shader",
      "swift",
      "ts",
      "tsx",
      "v",
      "vb",
      "xsjs",
      "xsjslib"
    ],
    "stylesheet": ["css", "less", "sass", "scss", "styl"],
    "template": [
      "ctp",
      "eco",
      "ecr",
      "ejs",
      "erb",
      "haml",
      "hamlc",
      "handlebars",
      "hbs",
      "hjs",
      "j2",
      "jade",
      "jinja2",
      "mustache",
      "njk",
      "pug",
      "slim",
      "twig",
      "xsd"
    ],
    "test": [
      {
        "names": ["spec", "test"],
        "templates": ["${name}.js", "${name}.jsx", "${name}.ts", "${name}.tsx"]
      }
    ],
    "todo": ["todo"]
  },
  "fileNames": {
    "changelog": [
      {
        "names": ["changelog", "history"],
        "templates": ["${name}", "${name}.md", "${name}.txt"]
      }
    ],
    "codeowners": ["authors", "codeowners", "owners"],
    "config": [
      ".editorconfig",
      ".gitattributes",
      ".gitkeep",
      ".gitmodules",
      ".htaccess",
      ".htmlvalidate.json",
      ".keep",
      ".npmrc",
      ".nvmrc",
      ".renovaterc",
      ".renovaterc.json",
      ".travis.yml",
      "cmakelists.txt",
      "cname",
      "config.yml",
      "dockerfile",
      "jakefile",
      "jenkinsfile",
      "labeler.yml",
      "makefile",
      "procfile",
      "release-drafter.yml",
      "renovate.json",
      "renovate.json5",
      "stale.yml",
      "tsconfig.json",
      {
        "names": ["babel"],
        "templates": [".${name}rc", ".${name}rc.json"]
      },
      {
        "names": ["eslint", "nyc", "prettier", "remark", "stylelint"],
        "templates": [
          ".${name}rc",
          ".${name}rc.json",
          ".${name}rc.yaml",
          ".${name}rc.yml"
        ]
      },
      {
        "names": ["markdownlint"],
        "templates": [
          ".${name}rc",
          ".${name}.yml",
          ".${name}.yaml",
          ".${name}.json"
        ]
      }
    ],
    "config-src": [
      "gruntfile.js",
      "gulpfile.js",
      {
        "names": ["eslint", "nyc", "prettier", "remark", "stylelint"],
        "templates": [".${name}rc.js"]
      },
      {
        "names": ["babel"],
        "templates": [".${name}rc.cjs", ".${name}rc.js", ".${name}rc.mjs"]
      }
    ],
    "ctags": ["tags"],
    "data": ["manifest.in", "pipfile"],
    "doc": [
      "contributing",
      "code_of_conduct",
      "issue_template",
      "pull_request_template"
    ],
    "ignore": [
      {
        "names": [
          "docker",
          "eslint",
          "git",
          "npm",
          "nyc",
          "prettier",
          "remark",
          "stylelint",
          "vscode"
        ],
        "templates": [".${name}ignore"]
      }
    ],
    "info": [
      {
        "names": ["readme"],
        "templates": ["${name}", "${name}.md", "${name}.txt"]
      }
    ],
    "license": [
      {
        "names": ["copying", "license", "license-code"],
        "templates": ["${name}", "${name}.md", "${name}.txt"]
      }
    ],
    "lock": [".vpkg-lock.json", "package-lock.json"],
    "ruby": ["capfile", "gemfile", "guardfile", "podfile", "rakefile"],
    "script": ["gradlew"],
    "src": ["cakefile"],
    "security": ["security", "security.md"],
    "support": ["funding.yml", "support", "support.md"],
    "todo": ["todo", "todo.md", "todo.txt"]
  },
  "folder": "folder",
