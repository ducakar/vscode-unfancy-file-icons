    "archive": [
      "7z",
      "bz2",
      "gz",
      "rar",
      "tar",
      "taz",
      "tbz",
      "tbz2",
      "tgz",
      "tlz",
      "txz",
      "tzo",
      "zip",
      "xz"
    ],
    "binary": ["dll", "exe", "pyc", "so"],
    "config": [
      "cake",
      "cfg",
      "conf",
      "directory",
      "dockerfile",
      "gradle",
      "gyp",
      "gypi",
      "ini",
      "mk",
      "properties",
      "pssc",
      "tern-project",
      "toml"
    ],
    "config-src": [
      "conf.js",
      "config.cjs",
      "config.js",
      "config.json",
      "config.mjs"
    ],
    "ctags": ["ctags", "gemtags", "tags"],
