      "cson",
      "edn",
      "env",
      "eyaml",
      "eyml",
      "json",
      "jsonc",
      "mod",
      "psd1",
      "webmanifest",
      "yaml",
      "yml"
    ],
    "data-markup": ["xaml", "xml", "xsl", "xslt"],
    "data-template": ["schema.json"],
    "database": ["dump", "sqlite", "sqlite3"],
    "database-src": ["dsql", "sql"],
    "debug": ["pdb"],
    "diff": ["diff", "patch"],
    "doc": ["dvi", "rdoc", "rst", "tex", "txt"],
    "script": [
      "bash",
      "bat",
      "cmd",
      "ksh",
      "ps1",
      "psm1",
      "psrc",
      "sh",
      "vbs",
      "vsh",
      "zsh"
    ],
    "font": ["eot", "otf", "ttf", "woff", "woff2"],
    "headers": ["d.ts", "h", "h++", "hpp", "hxx", "pyi"],
    "image": ["gif", "ico", "jpg", "png", "svg"],
    "image-src": ["afdesign", "ai", "indd", "psd", "sketch"],
    "linker": ["ilk"],
    "lock": ["lock"],
    "log": ["log"],
    "map": ["map"],
    "markdown": ["markdown", "md"],
    "markup": [
      "asp",
      "aspx",
      "cshtml",
      "htm",
